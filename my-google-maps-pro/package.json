{
	"name": "my-google-maps-pro",
	"version": "1.0.0",
	"type": "module",
	"description": "This is an example of an Apify Actor.",
	"dependencies": {
		"@crawlee/playwright": "^3.15.3",
		"apify": "^3.4.2",
		"crawlee": "^3.15.3",
		"playwright": "1.54.1"
	},
	"devDependencies": {
		"@apify/eslint-config": "^1.0.0",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"prettier": "^3.5.3",
		"@jest/globals": "^29.7.0",
		"jest": "^29.7.0",
		"jest-environment-node": "^29.7.0",
		"cross-env": "^7.0.3"
	},
	"scripts": {
		"start": "node src/main.js",
